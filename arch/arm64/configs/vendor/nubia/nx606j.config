# audio
CONFIG_SND_SOC_TAS2555_GENERIC=y
CONFIG_TAS2555_CODEC=y
CONFIG_TAS2555_REGMAP=y
CONFIG_TAS2555_MISC=y
CONFIG_NUBIA_TYPEC_HEADSET=y
CONFIG_TYPEC_AUDIO_ADAPTER_SWITCH=y

# charger
CONFIG_NUBIA_CHARGE_FEATURE=y
CONFIG_SMB1355_SLAVE_CHARGER=y
CONFIG_NUBIA_HW_STEP_CHARGE_FEATURE=y

# fingerprint
CONFIG_NUBIA_DRIVERS=y
CONFIG_NUBIA_FINGERPRINT=y
CONFIG_NUBIA_SENSORS_FP=y
CONFIG_NUBIA_SENSORS_FP_GF5288=y
CONFIG_NUBIA_SENSORS_FP_GF5288_PLATFORM=y

# hardware
CONFIG_NUBIA_HW_CONFIG_BY_GPIO=y
CONFIG_QCOM_RTB=y
CONFIG_QCOM_RTB_SEPARATE_CPUS=y

# lcd
CONFIG_NUBIA_LCD_DISP_PREFERENCE=y
CONFIG_NUBIA_LCD_KEEP=y
# CONFIG_NUBIA_LCD_BACKLIGHT_CURVE is not set

# led
CONFIG_NUBIA_BREATH_LEDS=y
CONFIG_NUBIA_WLED_OVP_FILTER=y

# nfc
CONFIG_NFC=y
CONFIG_SEC_NFC=y
CONFIG_SEC_NFC_PRODUCT_N5=y
CONFIG_SEC_NFC_IF_I2C=y

# others
CONFIG_LOG_BUF_SHIFT=19
CONFIG_CPU_FREQ_STAT=y
CONFIG_NUBIA_CGF_NOTIFY_EVENT=y
CONFIG_MEM_SHARE_QMI_SERVICE=y

# pstore
CONFIG_PSTORE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_RAM=y

# sensors
CONFIG_NUBIA_SENSORS=y
CONFIG_NUBIA_SENSORS_RGB_BH1745=y
CONFIG_NUBIA_VIRTUAL_PROXIMITY=y

# storage
CONFIG_NUBIA_READ_CONSOLE=y
CONFIG_CRYPTO_SHA512=y

# touchscreen
CONFIG_TOUCHSCREEN_NUBIA_SYNAPTICS_DSX=y
CONFIG_TOUCHSCREEN_NUBIA_SYNAPTICS_DSX_IIC=y
CONFIG_TOUCHSCREEN_NUBIA_SYNAPTICS_DSX_CORE=y
CONFIG_TOUCHSCREEN_NUBIA_SYNAPTICS_DSX_RMI_DEV=y
CONFIG_TOUCHSCREEN_NUBIA_SYNAPTICS_DSX_FW_UPDATE=y
CONFIG_TOUCHSCREEN_NUBIA_SYNAPTICS_DSX_TEST_REPORTING=y

# vibrator
CONFIG_FEATURE_ZTEMT_HAPTIC_VIBRATOR=y

# usb
CONFIG_NUBIA_USB_DISABLE_LINK_PM=y
CONFIG_NUBIA_USB_HIGH_SPEED=y
CONFIG_NUBIA_USB_PHY_TUNE=y

# Nubia configuration options
CONFIG_MACH_NUBIA_SDM845=y
CONFIG_MACH_NUBIA_NX606J=y
