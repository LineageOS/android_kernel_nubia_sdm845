# audio
CONFIG_SND_SOC_TFA9894_STEREO=y

# charger
CONFIG_NUBIA_CHARGE_FEATURE=y
# CONFIG_SMB1355_SLAVE_CHARGER is not set
CONFIG_NEO_DIRECT_CHARGE_SUPPORT=y
CONFIG_DIRECT_QC_COMPATIBLE_FEATURE=y
CONFIG_NEO_WP502X_CHARGER=y

# fingerprint
CONFIG_NUBIA_DRIVERS=y
CONFIG_NUBIA_FINGERPRINT=y
CONFIG_NUBIA_SENSORS_FP=y
CONFIG_NUBIA_SENSORS_FP_GF3266=y
CONFIG_SILEAD_FP_SPI=y
CONFIG_SILEAD_FP_PLATFORM=y

# f2fs
CONFIG_F2FS_STAT_FS=y
CONFIG_F2FS_IO_TRACE=y
CONFIG_NUBIA_F2FS_TRIM_STAT=y

# hardware
CONFIG_NUBIA_HW_CONFIG_BY_GPIO=y
CONFIG_NVMEM=y

# input
CONFIG_NUBIA_INPUT_KEYRESET=y
CONFIG_NUBIA_KEYBOARD_GAMESWITCH=y
CONFIG_NUBIA_CYPRESS_KEYBOARD_GAMEFIRE=y

# lcd
CONFIG_NUBIA_LCD_KEEP=y
CONFIG_NUBIA_LCD_DISP_PREFERENCE=y
# CONFIG_NUBIA_LCD_BACKLIGHT_CURVE is not set

# led
CONFIG_NUBIA_BREATH_LEDS=y
CONFIG_NUBIA_DOUBLE_LEDS=y
CONFIG_NUBIA_GPIO5_LEDS=y
CONFIG_NUBIA_LED=y
CONFIG_NUBIA_LED_AW22XXX=y
CONFIG_NUBIA_LED_TFA9XXX=y

# others
CONFIG_LOG_BUF_SHIFT=19
CONFIG_CPU_FREQ_STAT=y
CONFIG_NUBIA_CGF_NOTIFY_EVENT=y

# pstore
CONFIG_PSTORE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_RAM=y

# sensors
CONFIG_NUBIA_SENSORS_TMD2725=y

# storage
CONFIG_NUBIA_READ_CONSOLE=y
CONFIG_CRYPTO_SHA512=y

# touchscreen
CONFIG_NUBIA_TOUCHSCREEN_SYNAPTICS_DSX_2_7_0=y
CONFIG_NUBIA_TOUCHSCREEN_SYNAPTICS_DSX_I2C=y
CONFIG_NUBIA_TOUCHSCREEN_SYNAPTICS_DSX_CORE=y
CONFIG_NUBIA_TOUCHSCREEN_SYNAPTICS_DSX_RMI_DEV=y
CONFIG_NUBIA_TOUCHSCREEN_SYNAPTICS_DSX_FW_UPDATE=y
CONFIG_NUBIA_TOUCHSCREEN_SYNAPTICS_DSX_TEST_REPORTING=y
CONFIG_NUBIA_SENSORS=y

# vibrator
CONFIG_SND_SOC_TFA9874_OR_HAPTIC=y

# usb
CONFIG_NUBIA_USB_DISABLE_LINK_PM=y
CONFIG_NUBIA_USB_HIGH_SPEED=y
CONFIG_NUBIA_USB_PHY_TUNE=y

# Nubia configuration options
CONFIG_MACH_NUBIA_SDM845=y
CONFIG_MACH_NUBIA_NX619J=y
